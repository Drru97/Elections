@using ElectionLand.Models
@model VirtualDistrict
@{
    ViewBag.Title = "Додавання дільниці";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form asp-action="CreateDistrict" method="post">
    <div class="form-group">
        <label asp-for="Title" class="control-label">Назва</label>
        <input type="text" asp-for="Title" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Adress" class="control-label">Адреса</label>
        <input type="text" asp-for="Adress" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="VirtualCantonId" class="control-label">Округ</label>
        <input type="number" asp-for="VirtualCantonId" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>
