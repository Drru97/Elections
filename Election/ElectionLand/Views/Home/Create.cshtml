@model ElectionLand.Models.VirtualCanton
@{
    ViewBag.Title = "Добавление смартфона";
}
<form asp-action="create" asp-controller="district">
    <div class="form-group">
        <label asp-for="Title" class="control-label">Название</label>
        <input type="text" asp-for="Title" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Adress" class="control-label">Компания</label>
        <input type="text" asp-for="Adress" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Id" class="control-label">Цена</label>
        <input type="number" asp-for="Id" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>